msgid "Please call 'bisect_state' with at least one argument."
msgstr "请在调用 'bisect_state' 时跟至少一个参数。"

#: git-bisect.sh:244
#, sh-format
msgid "Bad rev input: $rev"
msgstr "输入坏的版本：$rev"

